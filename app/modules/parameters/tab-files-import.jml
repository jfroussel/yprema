											<option value="">non défini</option>
											<option j-for="(i, userColumn) in userColumns.card" :value="{{userColumn}}">{{userColumn}}</option>
										</select>
									</div>
								</div>
							</fieldset>
							
							<fieldset class="form-group required">
								<div class="row">
									<label class="col-md-4 control-label">référence société</label>
									<div class="col-md-8">
										<select name="driver_primary" data-rule-uniqselection="card-keys" autocomplete="off" required>
											<option value="">non défini</option>
											<option j-for="userColumn in userColumns.card" :value="{{userColumn}}">{{userColumn}}</option>
										</select>
									</div>
								</div>
							</fieldset>
						</div>
						
						<div class="import-card-area">
						
							<fieldset j-for="systemColumn in columns.card" class="form-group" :data-user-column="{{::systemColumn}}">
								<div class="row">
									<label class="col-md-4 control-label text-left">{{::systemColumn}}</label>
									<div class="col-md-8">
										<select :name="{{::systemColumn}}" type="text" class="select" data-live-search="true" data-rule-uniqselection="card-config" autocomplete="off">
											<option value="">non défini</option>
											<option j-for="(i, userColumn) in userColumns.card" :value="{{userColumn}}">{{userColumn}}</option>
										</select>
									</div>
								</div>
							</fieldset>
						
						</div>
						
					</div>
				</div>
			</form>
		</div>
		<div class="col-md-4">
			<form class="form-horizontal importmap" j-name="importmap.contact" name="contact">
				<div j-if="userColumns.contact" class="import-config">
					<div class="csv-contact">
						<h4>Mapping de vos champs passages</h4>

						<div class="import-keys">
							<fieldset class="form-group required import-config-primary">
								<div class="row">
									<label class="col-md-4 control-label">référence unique</label>
									<div class="col-md-8">
										<select name="primary" class="select" data-live-search="true"  data-rule-uniqselection="contact-keys" autocomplete="off" required>
											<option value="">non défini</option>
											<option j-for="(i, userColumn) in userColumns.contact" :value="{{userColumn}}">{{userColumn}}</option>
										</select>
									</div>
								</div>
							</fieldset>

							<!--<fieldset class="form-group required">-->
