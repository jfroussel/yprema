<div class="parameters-import-map">
	<div class="row">
		<div class="col-md-4">
			<form class="form-horizontal importmap" j-name="importmap.driver" name="driver">
				<div j-if="userColumns.driver" class="import-config">
					<div class="csv-driver">
						<h4>Mapping de vos champs chauffeurs</h4>
						
						<div class="import-keys">							
							<fieldset class="form-group required import-config-primary">
								<div class="row">
									<label class="col-md-4 control-label">référence unique</label>
									<div class="col-md-8">
										<select name="primary" class="select" data-live-search="true"  data-rule-uniqselection="driver-keys" autocomplete="off" required>
											<option value="">non défini</option>
											<option j-for="(i, userColumn) in userColumns.driver" :value="{{userColumn}}">{{userColumn}}</option>
										</select>
									</div>
								</div>
							</fieldset>			
						</div>
						
						<div class="import-driver-area">
						
							<fieldset j-for="systemColumn in columns.driver" class="form-group" :data-user-column="{{::systemColumn}}">
								<div class="row">
									<label class="col-md-4 control-label text-left">{{::systemColumn}}</label>
									<div class="col-md-8">
										<select :name="{{::systemColumn}}" type="text" class="select" data-live-search="true" data-rule-uniqselection="driver-config" autocomplete="off">
											<option value="">non défini</option>
											<option j-for="(i, userColumn) in userColumns.driver" :value="{{userColumn}}">{{userColumn}}</option>
										</select>
									</div>
								</div>
							</fieldset>
						
						</div>
						
					</div>
				</div>
			</form>
		</div>

		<div class="col-md-4">
			<form class="form-horizontal importmap" j-name="importmap.card"  name="card">
				<div j-if="userColumns.card" class="import-config">
					<div class="csv-card">
